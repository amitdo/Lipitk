<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003"
         DefaultTargets="runshaperec"> 
         <PropertyGroup>
            <LipiRoot>$(LIPI_ROOT)</LipiRoot>
         </PropertyGroup>
<ItemGroup>
    <StaticLibs Include="common;utils;shapereccommon;wordreccommon;featureextractorcommon"/>
</ItemGroup>
<Target Name="runshaperec" 
        DependsOnTargets="@(StaticLibs)">
    <VCBuild Projects="$(LIPI_ROOT)\src\util\run\runshaperec\windows\vc2005\runshaperec.vcproj"
             Rebuild="true"
             Configuration="Release|win32">
    </VCBuild>
</Target>
<Target Name="common" >
    <VCBuild Projects="$(LIPI_ROOT)\src\common\windows\vc2005\common.vcproj"
             Rebuild="true"
             Configuration="Release|win32">
    </VCBuild>
</Target>
<Target Name="utils" >
    <VCBuild Projects="$(LIPI_ROOT)\src\util\lib\windows\vc2005\utils.vcproj"
             Rebuild="true"
             Configuration="Release|win32">
    </VCBuild>
</Target>
<Target Name="shapereccommon" >
    <VCBuild Projects="$(LIPI_ROOT)\src\reco\shaperec\common\windows\vc2005\shapereccommon.vcproj"
             Rebuild="true"
             Configuration="Release|win32">
    </VCBuild>
</Target>
<Target Name="wordreccommon" >
    <VCBuild Projects="$(LIPI_ROOT)\src\reco\wordrec\common\windows\vc2005\wordreccommon.vcproj"
             Rebuild="true"
             Configuration="Release|win32">
    </VCBuild>
</Target>
<Target Name="featureextractorcommon" >
    <VCBuild Projects="$(LIPI_ROOT)\src\reco\shaperec\featureextractor\common\windows\vc2005\featureextractorcommon.vcproj"
             Rebuild="true"
             Configuration="Release|win32">
    </VCBuild>
</Target>
</Project>